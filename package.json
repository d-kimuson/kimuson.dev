{
  "name": "kimuemon-tech-blog",
  "version": "0.1.0",
  "private": true,
  "description": "My tech blog produced by gatsby",
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/techblog1/gatsby-blog"
  },
  "license": "0BSD",
  "author": "Kaito Kimura <dev.kimuson@gmail.com>",
  "main": "n/a",
  "scripts": {
    "blog:new": "git rev-parse --show-toplevel && ./bin/create.sh",
    "build": "gatsby build",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "gatsby develop -p 2828",
    "dev:codegen": "IS_CODEGEN=true gatsby develop",
    "lint": "yarn run eslint src --ext .tsx,.ts,.jsx,.js --fix && yarn run stylelint 'src/**/*.{scss,.css}' --fix",
    "post": "git rev-parse --show-toplevel && git add content && git commit -m '[auto] post article' && git push origin master",
    "sort:package": "npx sort-package-json",
    "start": "npm run develop",
    "try:prod": "yarn build && http-server public",
    "type:check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn type:check && yarn lint",
      "pre-push": "yarn build"
    }
  },
  "devDependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.1.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@graphql-codegen/cli": "^1.19.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.9",
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "@tsconfig/node14": "^1.0.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.6.1",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-share": "^3.0.3",
    "@types/swiper": "^5.4.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^9.8.4",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cssnano": "^4.1.10",
    "dayjs": "^1.9.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
