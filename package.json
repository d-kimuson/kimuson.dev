    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.7.0",
    "gatsby-plugin-sass": "^5.21.0",
    "gatsby-plugin-sharp": "^4.21.0",
    "gatsby-plugin-sitemap": "^5.21.0",
    "gatsby-plugin-typescript": "^4.21.0",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.27",
    "gatsby-remark-autolink-headers": "^4.11.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-images": "^6.21.0",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-source-filesystem": "^4.21.1",
    "gatsby-transformer-sharp": "^4.21.0",
    "http-server": "^13.0.2",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "lint-staged": "^11.2.6",
    "loadable-components": "^2.2.3",
    "mdx-utils": "^0.2.0",
    "modern-css-reset": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.5.1",
    "prismjs": "^1.28.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.2.2",
    "react-live": "^2.4.1",
    "react-share": "^4.4.0",
    "rimraf": "^3.0.2",
    "rss-parser": "^3.12.0",
    "sass": "^1.49.7",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.21.0",
    "subscriptions-transport-ws": "^0.11.0",
    "swiper": "^8.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.12.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "websocket": "^1.0.34"
  }
}
