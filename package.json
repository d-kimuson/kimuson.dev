    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-scss": "^3.19.0",
    "subscriptions-transport-ws": "^0.9.18",
    "swiper": "^6.4.15",
    "ts-jest": "^26.5.2",
    "ts-node": "^9.1.1",
    "tsc": "^1.20150623.0",
