    "gatsby-plugin-robots-txt": "^1.5.2",
    "gatsby-plugin-sass": "^2.3.13",
    "gatsby-plugin-sharp": "^2.6.29",
    "gatsby-plugin-sitemap": "^2.4.13",
    "gatsby-plugin-stylelint": "^3.2.0",
    "gatsby-plugin-typescript": "^2.4.18",
