{
  "name": "kimuson.dev",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "typecheck": "tsc -p ./tsconfig.src.json --noEmit",
    "lint:eslint": "eslint src --cache --ext .ts,.tsx",
    "lint:prettier": "prettier '**/*.{tsx,ts,mjs,cjs,js,json,css,scss,yml,yaml,md}' --check",
    "lint:cspell": "cspell lint './**/*.{tsx,ts,mjs,cjs,js,json,css,scss,yml,yaml,md}' --gitignore --cache",
    "lint": "pnpm /^lint:.*/",
    "fix:eslint": "pnpm lint:eslint --fix",
    "fix:prettier": "prettier '**/*.{tsx,ts,mjs,cjs,js,json,css,scss,yml,yaml,md}' --write",
    "fix": "pnpm /^fix:.*/",
    "preview": "astro preview",
    "astro": "astro",
    "run:lint-staged": "lint-staged --allow-empty -r",
    "setup:husky": "husky install",
    "postinstall": "pnpm setup:husky",
    "preinstall": "npx only-allow pnpm"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "pnpm@8.6.3",
  "engines": {
    "node": ">=18",
    "npm": "forbidden, use pnpm",
    "yarn": "forbidden, use pnpm",
    "pnpm": ">=8.6.3"
  },
  "overrides": {
    "react": "npm:@preact/compat@latest",
    "react-dom": "npm:@preact/compat@latest"
  },
  "dependencies": {
    "@astrojs/preact": "^2.0.3",
    "@astrojs/tailwind": "^3.0.1",
    "astro": "^2.6.3",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "jsdom": "^22.1.0",
    "preact": "^10.6.5",
    "remark-code-titles": "^0.1.2",
    "remeda": "^1.6.2",
    "rss-parser": "^3.13.0",
    "tailwindcss": "^3.3.2",
    "ts-array-length": "^0.1.2",
    "ts-pattern": "^4.1.4",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/jsdom": "^21.1.1",
    "@types/validator": "^13.7.12",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "commitizen": "^4.3.0",
    "cspell": "^6.26.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.8.0",
    "sass": "^1.62.1",
    "type-fest": "^3.6.0",
    "typescript": "^5.1.3",
    "validator": "^13.9.0"
  }
}
