    "postcss-flexbugs-fixes": "^4.2.1",
    "prettier": "^2.1.1",
    "prismjs": "^1.21.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.7",
