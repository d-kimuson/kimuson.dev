{
  "name": "kimuson.dev",
  "version": "0.1.0",
  "private": true,
  "description": "Personal blog 'kimuson.dev' created by gatsby.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/kimuson.dev.git"
  },
  "license": "0BSD",
  "author": "Kaito Kimura <dev.kimuson@gmail.com>",
  "main": "n/a",
  "scripts": {
    "blog:new": "git rev-parse --show-toplevel && ./bin/create.sh",
    "build": "run-s build:feed build:gatsby",
    "build:feed": "ts-node --project ./bin/tsconfig.json -r tsconfig-paths/register ./bin/fetch-feed.ts",
    "build:gatsby": "gatsby build",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "dev": "run-s build:feed dev:gatsby",
    "dev:codegen": "IS_CODEGEN=true gatsby develop",
    "dev:gatsby": "gatsby develop -p 2828",
    "typecheck": "tsc -p . --noEmit",
    "lint": "yarn typecheck && run-p lint:*",
    "lint:eslint": "eslint src --ext .tsx,.ts,.jsx",
    "lint:prettier": "prettier './**/*.{tsx,ts,jsx,json}' --check",
    "lint:stylelint": "stylelint 'src/**/*.{scss,.css}'",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier './**/*.{tsx,ts,jsx,json}' --write",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "now": "date +'%Y-%m-%dT%TZ'",
    "post": "git rev-parse --show-toplevel && git add content && git commit -m '[article] post article' && git push origin master",
    "start": "yarn build && http-server public -p 80",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-brands-svg-icons": "^5.15.2",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@graphql-codegen/cli": "^1.21.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-operations": "^1.17.14",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tsconfig/node14": "^1.0.0",
    "@types/mdx-js__react": "^1.5.3",
    "@types/node": "^14.14.31",
    "@types/ramda": "^0.27.38",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-share": "^3.0.3",
    "@types/swiper": "^5.4.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "animate.css": "^4.1.1",
    "autoprefixer": "^9",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cssnano": "^4.1.10",
    "dayjs": "^1.10.4",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-react": "^7.22.0",
    "gatsby": "^2.32.8",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-feed-mdx": "^1.1.0",
    "gatsby-plugin-fontawesome-css": "^1.0.0",
    "gatsby-plugin-google-analytics": "^2.11.0",
    "gatsby-plugin-graphql-codegen": "^2.7.1",
    "gatsby-plugin-manifest": "^2.12.1",
    "gatsby-plugin-mdx": "^1.10.1",
    "gatsby-plugin-netlify": "^2.11.0",
    "gatsby-plugin-next-seo": "^1.7.0",
    "gatsby-plugin-offline": "^3.10.2",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^3.2.0",
    "gatsby-plugin-sharp": "^2.14.3",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-typescript": "^2.12.1",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-images": "^3.11.1",
