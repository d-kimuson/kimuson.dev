    "husky": "^7.0.4",
    "jest": "^27.4.3",
    "lint-staged": "^11.2.6",
    "loadable-components": "^2.2.3",
    "mdx-utils": "^0.2.0",
    "modern-css-reset": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "prettier": "^2.5.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
