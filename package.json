{
  "name": "kimuson.dev",
  "version": "0.1.0",
  "private": true,
  "description": "Personal blog 'kimuson.dev' created by gatsby.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/kimuson.dev.git"
  },
  "license": "0BSD",
  "author": "d-kimuson <dev.kimuson@gmail.com>",
  "scripts": {
    "dev": "run-s build:feed dev:gatsby",
    "dev:gatsby": "gatsby develop -p 2828",
    "start": "yarn build && http-server public -p 80 --brotli",
    "build": "run-s build:feed build:gatsby",
    "build:feed": "ts-node --project ./bin/tsconfig.json -r tsconfig-paths/register ./bin/fetch-feed.ts",
    "build:gatsby": "gatsby build",
    "prebuild": "rimraf public",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "typecheck": "tsc -p . --noEmit",
    "lint": "yarn typecheck && run-p lint:*",
    "lint:eslint": "eslint src --cache --ext .tsx,.ts,.jsx",
    "lint:prettier": "prettier './**/*.{tsx,ts,jsx,json,md,mdx}' --check",
    "lint:stylelint": "stylelint --cache 'src/**/*.{scss,.css}'",
    "lint:cspell": "cspell lint './**/*.{ts,js,json,md,yml,yaml}' --gitignore --cache",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier './**/*.{tsx,ts,jsx,json,md,mdx}' --write",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "lint-staged": "lint-staged --allow-empty",
    "now": "date +'%Y-%m-%dT%TZ'",
    "blog:new": "git rev-parse --show-toplevel && ./bin/create.sh article",
    "work:new": "git rev-parse --show-toplevel && ./bin/create.sh work",
    "deploy": "git rev-parse --show-toplevel && git add content && git commit -m '[article] post article' && git push origin master",
    "test": "jest",
    "prepare": "husky install",
    "postinstall": "yarn repair:sharp",
    "repair:sharp": "npm rebuild --verbose sharp"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "gatsby-plugin-feed": "^4.21.0",
    "preact": "^10.6.5",
    "preact-render-to-string": "^5.1.19",
    "prism-react-renderer": "^1.3.5",
    "ramda": "^0.28.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.4",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tsconfig/node18-strictest": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@types/mdx-js__react": "^1.5.5",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "@types/node": "^18.7.18",
    "@types/ramda": "^0.27.64",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.5",
    "@types/react-share": "^4.0.0",
    "@types/swiper": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitizen": "^4.2.5",
    "cspell": "^6.7.0",
    "cssnano": "^5.0.16",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.22",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "gatsby": "^4.21.1",
    "gatsby-plugin-fontawesome-css": "^1.1.0",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-image": "^2.21.0",
    "gatsby-plugin-manifest": "^4.21.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-next-seo": "^1.10.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-preact": "^6.21.0",
