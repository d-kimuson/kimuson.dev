    "prismjs": "^1.23.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-helmet-async": "^1.0.7",
    "react-live": "^2.2.3",
    "react-share": "^4.3.1",
    "rss-parser": "^3.12.0",
