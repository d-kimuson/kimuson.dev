{
  "name": "kimuson.dev",
  "version": "0.1.0",
  "private": true,
  "description": "Personal blog 'kimuson.dev' created by gatsby.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/d-kimuson/kimuson.dev.git"
  },
  "license": "0BSD",
  "author": "Kaito Kimura <dev.kimuson@gmail.com>",
  "main": "n/a",
  "scripts": {
    "dev": "run-s build:feed dev:gatsby",
    "dev:gatsby": "gatsby develop -p 2828",
    "start": "yarn build && http-server public -p 80 --brotli",
    "build": "run-s build:feed build:gatsby",
    "build:feed": "ts-node --project ./bin/tsconfig.json -r tsconfig-paths/register ./bin/fetch-feed.ts",
    "build:gatsby": "gatsby build",
    "prebuild": "rimraf public",
    "clean": "gatsby clean",
    "codegen": "graphql-codegen --config codegen.yml",
    "typecheck": "tsc -p . --noEmit",
    "lint": "yarn typecheck && run-p lint:*",
    "lint:eslint": "eslint src --cache --ext .tsx,.ts,.jsx",
    "lint:prettier": "prettier './**/*.{tsx,ts,jsx,json,md,mdx}' --check",
    "lint:stylelint": "stylelint --cache 'src/**/*.{scss,.css}'",
    "fix": "run-p fix:*",
    "fix:eslint": "yarn lint:eslint --fix",
    "fix:prettier": "prettier './**/*.{tsx,ts,jsx,json,md,mdx}' --write",
    "fix:stylelint": "yarn lint:stylelint --fix",
    "lint-staged": "lint-staged --allow-empty",
    "now": "date +'%Y-%m-%dT%TZ'",
    "blog:new": "git rev-parse --show-toplevel && ./bin/create.sh article",
    "work:new": "git rev-parse --show-toplevel && ./bin/create.sh work",
    "deploy": "git rev-parse --show-toplevel && git add content && git commit -m '[article] post article' && git push origin master",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "preact": "^10.6.3",
    "preact-render-to-string": "^5.1.19",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@graphql-codegen/cli": "^2.3.0",
    "@graphql-codegen/typescript": "^2.4.1",
    "@graphql-codegen/typescript-operations": "^2.2.1",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.0.3",
    "@types/mdx-js__react": "^1.5.5",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.12",
    "@types/ramda": "^0.27.60",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@types/react-share": "^4.0.0",
    "@types/swiper": "^5.4.3",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "animate.css": "^4.1.1",
    "autoprefixer": "^10.4.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "cssnano": "^5.0.12",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "esbuild": "^0.14.2",
    "esbuild-jest": "^0.5.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "fibers": "^5.0.0",
    "gatsby": "^3.14.6",
    "gatsby-plugin-feed-mdx": "^1.1.0",
    "gatsby-plugin-fontawesome-css": "^1.1.0",
    "gatsby-plugin-google-adsense": "^1.1.3",
    "gatsby-plugin-google-analytics": "^3.14.0",
    "gatsby-plugin-image": "^1.14.1",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-mdx": "^2.14.0",
    "gatsby-plugin-next-seo": "^1.9.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-preact": "^5.14.0",
    "gatsby-plugin-purgecss": "^6.1.0",
    "gatsby-plugin-remove-console": "^0.0.2",
    "gatsby-plugin-robots-txt": "^1.6.14",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.3",
    "gatsby-plugin-sitemap": "^4.10.0",
